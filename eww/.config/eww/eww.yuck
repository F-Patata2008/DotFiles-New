;; ~/.config/eww/eww.yuck

;; Define a simple window named "test_window"
(defwindow test_window
  :monitor 0
  :geometry (geometry :x "10px"
                      :y "10px"
                      :width "200px"
                      :height "50px")
  :stacking "fg"
  :wm-ignore false
  (hello_widget))

;; Define a simple widget with some text
(defwidget hello_widget []
  (label :text "Hello, Eww!" :halign "center" :valign "center"))
